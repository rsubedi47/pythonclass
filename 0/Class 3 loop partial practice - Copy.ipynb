    "## Control Statements in While Loop\n",
    "\n",
    "i = 0  # a. initialize\n",
    "while i <= 10:   # b. condition\n",
    "    if i == 5:\n",
    "        break\n",
    "    print(i)\n",
    "    i = i+1    # c. increment"
